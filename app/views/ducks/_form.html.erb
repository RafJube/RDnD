<%= simple_form_for(@duck) do |f| %>
  <%= f.input :name %>
  <%= f.input :skills, as: :check_boxes, collection: Skill.all %>
  <%= f.input :photo, as: :file %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.submit %>
<% end %>

<%= simple_form_for [ @rental, @duck ] do |f| %>
  <%= f.input :date_start %>
  <%= f.input :date_end %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>